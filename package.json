    "@nuxtjs/stylelint-module": "^4.1.0",
    "@vue/test-utils": "^1.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^28.1.3",
    "eslint": "^7.32.0",
